//obj denoted by {}, key-value pair
let obj={
    "Name": "John",
    "Age": 30,
    "City": "New York"
}

let obj1= {
    "num1" : 10,
    "num2" : 20,
    "num3" : 30
}
console.log(obj, obj1)
//how to access any key
//2 ways - obj.key syntax
console.log(obj.Age)
//obj[" key"] syntax
console.log(obj1["num1"])


//how to update any key

